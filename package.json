{
  "name": "weekend",
  "version": "1.0.0",
  "description": "",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "electron .",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix",
    "format": "prettier --write .",
    "build": "vite build && electron-builder",
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps || true",
    "test:benchmark": "ts-node scripts/benchmark.ts run",
    "test:benchmark:history": "ts-node scripts/benchmark.ts history",
    "test:benchmark:latest": "ts-node scripts/benchmark.ts latest",
    "test:benchmark:compare": "ts-node scripts/benchmark.ts compare",
    "test:structure": "ts-node scripts/test-structure.ts",
    "check:ui-imports": "node scripts/check-imports.js",
    "test:unit": "vitest --config vitest.config.cjs --run --watch=false",
    "test:structure:tune": "ts-node scripts/auto-tune-structure.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tonejs/midi": "^2.0.27",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "better-sqlite3": "^12.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "easymidi": "^3.1.0",
    "electron": "^39.2.0",
    "essentia.js": "^0.1.3",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "lucide-react": "^0.554.0",
    "node-osc": "^11.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "sql.js": "^1.13.0",
    "tailwind-merge": "^3.4.0",
    "tonal": "^4.6.2",
    "vite": "^7.2.2",
    "wav-decoder": "^1.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^8.2.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vitest": "^1.6.1"
  },
  "vitest": {
    "test": {
      "environment": "jsdom",
      "globals": true,
      "include": [
        "src/**/*.{test,spec}.{js,jsx,ts,tsx}"
      ]
    }
  }
}
