{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "."
  },
  "include": [
    "scripts/**/*",
    "benchmarks/**/*",
    "src/**/*"
  ]
}

